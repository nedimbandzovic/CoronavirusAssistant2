<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coronavirusassistant">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Coronavirus Assistant"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoronavirusAssistant"
        android:usesCleartextTraffic="true"
        android:requestLegacyExternalStorage="true"
        >

        <activity android:name=".pfizer"></activity>
        <activity android:name=".jj" />
        <activity android:name=".sinovac" />
        <activity android:name=".sinopharm" />
        <activity android:name=".moderna" />
        <activity android:name=".astrazeneca" />
        <activity android:name=".SputnikV" />
        <activity android:name=".vaccines" />
        <activity android:name=".mcpbih" />
        <activity android:name=".mzfbih" />
        <activity android:name=".mzks" />
        <activity android:name=".covid19ba" />
        <activity android:name=".HomeScreen" />
        <activity
            android:name=".MainActivity"
            android:label="Coronavirus Assistant" />
        <activity
            android:name=".Login"
            android:label="Coronavirus Assistant">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>